<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SIOBundle\Entity\Entreprise" table="entreprise">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idutilisateur" type="integer" column="idUtilisateur" nullable="false"/>
    <field name="nom" type="string" column="nom" length="255" nullable="false"/>
    <field name="type" type="string" column="type" length="255" nullable="false"/>
    <field name="description" type="text" column="description" nullable="false"/>
    <field name="adresse" type="string" column="adresse" length="255" nullable="false"/>
    <field name="adresse2" type="string" column="adresse2" length="255" nullable="false"/>
    <field name="cp" type="string" column="cp" length="5" nullable="false"/>
    <field name="ville" type="string" column="ville" length="50" nullable="false"/>
    <field name="tel1" type="string" column="tel1" length="20" nullable="false"/>
    <field name="tel2" type="string" column="tel2" length="20" nullable="false"/>
  </entity>
</doctrine-mapping>
